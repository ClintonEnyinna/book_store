<h1>Log In</h1>

<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>
  <br />

  <%= f.label :password %>
  <%= f.password_field :password %>
  <br />

  <%= f.submit 'Log In' %>
<% end %>
<%= link_to 'Sign up as buyer', new_buyer_path %>
<br />
<%= link_to 'Sign up as seller', new_seller_path %>